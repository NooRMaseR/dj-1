{
    "builds": [
        {
            "src": "dj_test/wsgi.py",
            "use": "@vercel/python",
            "config": {"maxLambdaSize": "15mb", "runtime": "python3.12"}
        }
    ],
    "routes": [
        {
            "src": "/.(.*)",
            "dest": "dj_test/wsgi.py"
        }
    ]
}